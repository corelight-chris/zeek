#
# @TEST-EXEC: bro -b %INPUT
# @TEST-EXEC: btest-diff test.log
#
# Testing all possible types.

redef LogAscii::use_json = F;
redef LogAscii::size_limit_hint = 2000;

module Test;

export {
	redef enum Log::ID += { LOG };

	type Log: record {
		a: count;
		s: string &optional;
		sc: set[count] &optional;
		ss: set[string] &optional;
		b: bool;
	} &log;
}

event bro_init()
{
	Log::create_stream(Test::LOG, [$columns=Log]);

	local long_string = "";
	local large_set: set[count];

	local i = 0;
	while ( i < 10000 ) 
		{
		add large_set[i];
		i = i + 1;
		}

	local ss: set[string];
	add ss["Foo"];
	add ss["Bar"];
	add ss[long_string];

	Log::write(Test::LOG, [
		$a=42,
		$s=long_string,
		$b=T]);

	Log::write(Test::LOG, [
		$a=42,
		$ss=ss,
		$b=T]);

	Log::write(Test::LOG, [
		$a=42,
		$sc=large_set,
		$b=T]);

	Log::write(Test::LOG, [
		$a=42,
		$s=long_string,
		$sc=large_set,
		$b=T]);

	Log::write(Test::LOG, [
		$a=42,
		$b=T]);
}

